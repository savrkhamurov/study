using System;

namespace sequence
{
    class Program
    {
        static void Main(string[] args)
        {
            int seed = 7;
            int incrementNumber = 7;
            int limit = 98;
            for (int i = seed; i <= limit; i+=incrementNumber)
            {
                Console.WriteLine(i);
            }
            Console.ReadLine();
        }
    }
}

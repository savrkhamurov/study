using System;

namespace leorn1
{
    class Program
    {
        static void Main(string[] args)
        {
            Random randomNumber = new Random();
            int naturalNumber = randomNumber.Next(1, 28);
            int naturalNumbersSum = 0;
            Console.WriteLine($"Число N:{naturalNumber}");

            for (int i = naturalNumber; i < 1000; i+=naturalNumber)
            {
                if (i > 99)
                {
                    naturalNumbersSum += 1;
                }
            }

            Console.WriteLine($"Количество натуральных чисел кратных N:{naturalNumbersSum}");
        }
    }
}
